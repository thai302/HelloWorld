<?xml version = "1.0" encoding = "utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name = "com.kitcut.entity.Vt" table = "VT">

        <meta attribute = "class-description">
            This class contains the employee detail.
        </meta>

        <composite-id>
            <key-property name="hdId" type="int" column="hd_id"></key-property>
            <key-property name="vtId" type="int" column="vt_id"></key-property>
        </composite-id>
        <property name = "tenVt" column = "ten_vt" type = "string"/>

    </class>
</hibernate-mapping>